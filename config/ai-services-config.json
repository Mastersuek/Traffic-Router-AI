{
  "providers": {
    "openai": {
      "name": "OpenAI",
      "baseUrl": "https://api.openai.com",
      "apiKeyHeader": "Authorization",
      "apiKeyPrefix": "Bearer ",
      "requiresGeolocation": true,
      "supportedRegions": ["US", "EU"],
      "rateLimit": {
        "requestsPerMinute": 60,
        "requestsPerHour": 1000,
        "tokensPerMinute": 150000,
        "tokensPerHour": 1000000
      },
      "endpoints": {
        "chat": "/v1/chat/completions",
        "completions": "/v1/completions",
        "embeddings": "/v1/embeddings",
        "images": "/v1/images/generations"
      },
      "specialHeaders": {
        "OpenAI-Organization": "${OPENAI_ORG_ID}",
        "OpenAI-Beta": "assistants=v1"
      },
      "optimization": {
        "enableCaching": true,
        "cacheTTL": 300000,
        "maxTokens": 4000,
        "disableStreaming": true
      }
    },
    "anthropic": {
      "name": "Anthropic Claude",
      "baseUrl": "https://api.anthropic.com",
      "apiKeyHeader": "x-api-key",
      "requiresGeolocation": true,
      "supportedRegions": ["US"],
      "rateLimit": {
        "requestsPerMinute": 50,
        "requestsPerHour": 800,
        "tokensPerMinute": 100000,
        "tokensPerHour": 500000
      },
      "endpoints": {
        "messages": "/v1/messages",
        "complete": "/v1/complete"
      },
      "specialHeaders": {
        "anthropic-version": "2023-06-01",
        "anthropic-beta": "messages-2023-12-15"
      },
      "optimization": {
        "enableCaching": true,
        "cacheTTL": 300000,
        "maxTokens": 4000,
        "disableStreaming": true
      }
    },
    "google": {
      "name": "Google AI",
      "baseUrl": "https://generativelanguage.googleapis.com",
      "apiKeyHeader": "Authorization",
      "apiKeyPrefix": "Bearer ",
      "requiresGeolocation": true,
      "supportedRegions": ["US", "EU"],
      "rateLimit": {
        "requestsPerMinute": 100,
        "requestsPerHour": 2000
      },
      "endpoints": {
        "generateContent": "/v1beta/models/gemini-pro:generateContent",
        "embedContent": "/v1beta/models/embedding-001:embedContent"
      },
      "optimization": {
        "enableCaching": false,
        "maxTokens": 2048
      }
    },
    "huggingface": {
      "name": "Hugging Face",
      "baseUrl": "https://api-inference.huggingface.co",
      "apiKeyHeader": "Authorization",
      "apiKeyPrefix": "Bearer ",
      "requiresGeolocation": false,
      "supportedRegions": ["GLOBAL"],
      "rateLimit": {
        "requestsPerMinute": 30,
        "requestsPerHour": 500
      },
      "endpoints": {
        "inference": "/models/{model_id}"
      },
      "optimization": {
        "enableCaching": true,
        "cacheTTL": 600000
      }
    }
  },
  "routing": {
    "rules": [
      {
        "pattern": "*.openai.com/*",
        "provider": "openai",
        "priority": 100,
        "requiresProxy": true,
        "virtualLocation": "US"
      },
      {
        "pattern": "*.anthropic.com/*",
        "provider": "anthropic",
        "priority": 100,
        "requiresProxy": true,
        "virtualLocation": "US"
      },
      {
        "pattern": "*generativelanguage.googleapis.com/*",
        "provider": "google",
        "priority": 90,
        "requiresProxy": true,
        "virtualLocation": "US"
      },
      {
        "pattern": "*huggingface.co/*",
        "provider": "huggingface",
        "priority": 70,
        "requiresProxy": false
      }
    ],
    "fallback": {
      "provider": "huggingface",
      "requiresProxy": false
    }
  },
  "security": {
    "blockedRegions": ["RU", "CN", "IR", "KP"],
    "allowedOrigins": ["*"],
    "rateLimitByIP": {
      "enabled": true,
      "requestsPerMinute": 100,
      "requestsPerHour": 1000
    },
    "apiKeyValidation": {
      "enabled": true,
      "requireValidKey": true
    }
  },
  "monitoring": {
    "enableMetrics": true,
    "logRequests": true,
    "logResponses": false,
    "alertThresholds": {
      "errorRate": 0.05,
      "responseTime": 10000,
      "rateLimitHits": 0.8
    }
  }
}
