{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:ntfs": "bash scripts/build-ntfs-fix.sh",
    "build:quick": "bash scripts/quick-build.sh",
    "fix:permissions": "bash scripts/fix-permissions.sh",
    "fix:all": "bash scripts/fix-all-issues.sh",
    "dev": "cross-env PORT=13000 next dev",
    "dev:proxy": "cross-env AI_PROXY_PORT=13081 node server/ai-proxy-server.js",
    "dev:enhanced-proxy": "cross-env AI_PROXY_PORT=13081 node server/enhanced-ai-proxy-server.js",
    "dev:recovery-agent": "python3 scripts/start-recovery-agent.py",
    "dev:recovery-interactive": "python3 scripts/start-recovery-agent.py --interactive",
    "dev:monitor": "cross-env MONITORING_PORT=13082 node server/monitoring-server.js",
    "dev:mcp": "cross-env MCP_SERVER_PORT=3001 node server/mcp-integration-server.js",
    "dev:youtube": "cross-env YOUTUBE_CACHE_PORT=13083 node server/youtube-cache-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:proxy\" \"npm run dev:monitor\" \"npm run dev:mcp\" \"npm run dev:youtube\"",
    "build:ts": "tsc --build",
    "build:ts:watch": "tsc --watch",
    "compile:lib": "tsc lib/*.ts --outDir lib/compiled --target ES2020 --module commonjs",
    "lint": "next lint",
    "start": "next start",
    "proxy:start": "node server/ai-proxy-server.js",
    "monitor:start": "node server/monitoring-server.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.production.config.js",
    "stop:prod": "pm2 stop ecosystem.production.config.js",
    "restart:prod": "pm2 restart ecosystem.production.config.js",
    "reload:prod": "pm2 reload ecosystem.production.config.js",
    "logs:prod": "pm2 logs",
    "monit:prod": "pm2 monit",
    "build:prod": "NODE_ENV=production npm run build:ts && npm run build",
    "build:prod:optimized": "NODE_ENV=production GENERATE_SOURCEMAP=false npm run build:ts && npm run build",
    "build:prod:analyze": "NODE_ENV=production ANALYZE_BUNDLE=true npm run build",
    "prebuild": "npm run build:ts",
    "postbuild": "npm run build:verify",
    "build:verify": "node -e \"console.log('Build verification passed')\"",
    "validate:prod": "node scripts/validate-production-config.js",
    "prestart:prod": "npm run validate:prod",
    "deploy:prod": "bash scripts/deploy-production.sh",
    "setup:prod": "sudo bash scripts/setup-production.sh",
    "health:check": "curl -f http://localhost:3000/api/health",
    "docker:build": "docker build -f Dockerfile.production -t traffic-router:latest .",
    "docker:run": "docker-compose -f docker-compose.production.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f"
  },
